	Ty = {BOOK},
	Url = {https://www.cambridge.org/core/books/traitmediated-indirect-interactions/0D12F46495ACD5E45622BCA1205C9A24},
	Year = {2012},
	Bdsk-Url-1 = {https://www.cambridge.org/core/books/traitmediated-indirect-interactions/0D12F46495ACD5E45622BCA1205C9A24},
