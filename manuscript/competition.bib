	Journal = {The American Naturalist},
	Number = {3},
	Pages = {432--456},
	Publisher = {{$[$}University of Chicago Press, American Society of Naturalists{$]$}},
	Title = {Long-Term Experimental Evolution in Escherichia coli. II. Changes in Life-History Traits During Adaptation to a Seasonal Environment},
	Ty = {JOUR},
	Url = {www.jstor.org/stable/2462954},
	Volume = {144},
