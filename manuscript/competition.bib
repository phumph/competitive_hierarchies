	Mesh = {Actinobacteria; Antibiosis; Archaea; Bacteria; Beta vulgaris; Metagenome; Microbial Consortia; Molecular Sequence Data; Oligonucleotide Array Sequence Analysis; Peptide Synthases; Plant Diseases; Plant Roots; Proteobacteria; Pseudomonadaceae; Rhizoctonia; Rhizosphere; Soil Microbiology},
	Month = {May},
