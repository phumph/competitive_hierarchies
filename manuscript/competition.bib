	Journal = {Oikos},
	Number = {7},
	Pages = {1201-1209},
	Title = {Preemption of space can lead to intransitive coexistence of competitors},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1600-0706.2009.18068.x},
	Volume = {119},
	Year = {2010},
	Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1600-0706.2009.18068.x},
