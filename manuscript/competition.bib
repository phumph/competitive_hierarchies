	Keywords = {anisotropic environments; interaction networks; microbial communities; microbial ecology; reaction−diffusion models},
	Mesh = {Computer Simulation; Microbial Consortia; Models, Biological},
	Month = {01},
	Number = {2},
	Pages = {379-388},
	Pmc = {PMC6329963},
	Pmid = {30593565},
