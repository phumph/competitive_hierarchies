@article{Godoy17a,
	Annote = {doi: 10.1002/ecy.1782},
	Author = {Godoy, Oscar and Stouffer, Daniel B. and Kraft, Nathan J. B. and Levine, Jonathan M.},
	Booktitle = {Ecology},
	Da = {2017/05/01},
	Date = {2017/05/01},
	Date-Added = {2020-04-07 08:19:26 -0600},
