	Issn = {00664162},
	Journal = {Annual Review of Ecology and Systematics},
	Keywords = {Biological sciences -- Ecology -- Ecological processes ; Biological sciences -- Ecology -- Population ecology ; Biological sciences -- Ecology -- Biological taxonomies ; Biological sciences -- Biology -- Ecological processes ; Biological sciences -- Ecology -- Ecological processes ; Biological sciences -- Ecology -- Ecosystems ; Biological sciences -- Ecology -- Ecological processes ; Biological sciences -- Ecology -- Financial economics ; Economics -- Economic disciplines -- Human societies ; Behavioral sciences -- Sociology -- Human societies},
	Language = {eng},
	Pages = {443--466},
	Publisher = {Annual Reviews Inc.},
	Title = {The Nature and Consequences of Indirect Effects in Ecological Communities},
	Volume = {25},
