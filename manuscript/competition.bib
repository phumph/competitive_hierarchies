	Doi = {10.1038/ismej.2007.113},
	Journal = {ISME J},
	Journal-Full = {The ISME journal},
	Mesh = {Bacterial Proteins; Beta vulgaris; Cucumis; Ecosystem; Genotype; Ice; Lettuce; Phylogeny; Plant Diseases; Pseudomonas syringae; Rain; Rivers; Snow; Tobacco},
	Month = {Mar},
	Number = {3},
	Pages = {321-34},
	Pmid = {18185595},
	Pst = {ppublish},
	Title = {The life history of the plant pathogen Pseudomonas syringae is linked to the water cycle},
