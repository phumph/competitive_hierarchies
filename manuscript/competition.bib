	Journal = {Oikos},
	Number = {10},
	Pages = {1393-1400},
	Title = {Linked exploitation and interference competition drives the variable behavior of a classic predator--prey system},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1600-0706.2013.00418.x},
	Volume = {122},
	Year = {2013},
